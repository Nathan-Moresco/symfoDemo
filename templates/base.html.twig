<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" class="rounded-full" href="{{ asset('images/gitLogo-round.png') }}">
        
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}

        <script src="https://kit.fontawesome.com/fe5697ca0b.js" crossorigin="anonymous"></script>
    </head>
    <body>
        {% include "includes/navbar.html.twig" %}

        <div class="flex flex-col justify-center items-center p-12 gap-6">
            {% block content %}{% endblock %}
        </div>

    </body>
</html>
